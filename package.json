{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-objects": "cd nova-components/Objects && npm run dev",
        "build-objects-prod": "cd nova-components/Objects && npm run prod",
        "build-nova-translaable": "cd nova-components/NovaTranslaable && npm run dev",
        "build-nova-translaable-prod": "cd nova-components/NovaTranslaable && npm run prod",
        "build-translatable": "cd nova-components/Translatable && npm run dev",
        "build-translatable-prod": "cd nova-components/Translatable && npm run prod",
        "build-refferals": "cd nova-components/Referrals && npm run dev",
        "build-refferals-prod": "cd nova-components/Referrals && npm run prod",
        "build-chat": "cd nova-components/Chat && npm run dev",
        "build-chat-prod": "cd nova-components/Chat && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-svg-loader": "^0.11.0",
        "vue-template-compiler": "^2.5.21"
    },
    "dependencies": {
        "bulma": "^0.7.2",
        "inline-svg": "^2.2.3",
        "laravel-echo": "^1.5.2",
        "socket.io-client": "^2.2.0",
        "swiper": "^4.4.6",
        "v-tooltip": "^2.0.0-rc.33",
        "velocity-animate": "^2.0.5",
        "vue-multiselect": "^2.1.3",
        "vue-simple-svg": "^1.3.2",
        "vue-slider-component": "^2.8.5",
        "vuebar": "0.0.20",
        "vuelidate": "^0.7.4"
    }
}
