{
    "name": "marketplace/business",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Marketplace\\Business\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marketplace\\Business\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
