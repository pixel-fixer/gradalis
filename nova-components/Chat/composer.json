{
    "name": "marketplace/chat",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Marketplace\\Chat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marketplace\\Chat\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
